{
    "name": "myclassroom",
    "version": "0.1.0",
    "private": true,
    "description": "Open Source Classroom Polling Software",
    "repository": {
      "type": "git",
      "url": "https://github.com/OSU-MC/MyClassroom/"
    },
    "workspaces": [
        "server",
        "client"
    ],
    "scripts": {
        "config": "npm run config --workspace=server && npm run config --workspace=client && npm run setup-db --workspace=server && npm run migrate --workspace=server",
        "reset-db": "npm run unmigrate --workspace=server",
        "start": "concurrently \"npm start --workspace=server\" \"npm start --workspace=client\""
    },
    "keywords": [],
    "author": "",
    "license": "GPL-3.0-or-later",
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}
