{
    "name": "myclassroom",
    "version": "0.1.0",
    "private": true,
    "description": "Open Source Classroom Polling Software",
    "repository": {
      "type": "git",
      "url": "https://github.com/OSU-MC/MyClassroom/"
    },
    "workspaces": [
        "server",
        "client"
    ],
    "scripts": {
        "setup": "npm run setup --workspace=server && npm run setup --workspace=client",
        "migrate-db": "npm run migrate --workspace=server",
        "migrate-db:test": "npm run migrate:test --workspace=server",
        "unmigrate-db": "npm run unmigrate --workspace=server",
        "seed-db": "npm run seed --workspace=server",
        "seed-db:test": "npm run seed:test --workspace=server",
        "unseed-db": "npm run unseed --workspace=server",
        "start": "concurrently \"npm start --workspace=server\" \"npm start --workspace=client\""
    },
    "keywords": [],
    "author": "",
    "license": "GPL-3.0-or-later",
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}
