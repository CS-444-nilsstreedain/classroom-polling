{
    "name": "myclassroom",
    "version": "0.1.0",
    "private": true,
    "description": "Open Source Classroom Polling Software",
    "repository": {
      "type": "git",
      "url": "https://github.com/OSU-MC/MyClassroom/"
    },
    "workspaces": [
        "server",
        "client"
    ],
    "scripts": {
        "config": "./scripts/config.sh",
        "reset-db": "npm run drop -w=server && npm run drop:dev -w=server && npm run drop:test -w=server",
        "start": "concurrently \"npm start -w=server\" \"npm start -w=client\"",
        "start:dev": "concurrently \"npm run start:dev -w=server\" \"npm start -w=client\"",
        "start:test": "concurrently \"npm run start:test -w=server\" \"npm start -w=client\""
    },
    "keywords": [],
    "author": "",
    "license": "GPL-3.0-or-later",
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}
